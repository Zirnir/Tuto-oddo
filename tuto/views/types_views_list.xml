<?xml version="1.0"?>
<odoo>
    <record id="views_types_form" model="ir.ui.view">
        <field name="name">types.form</field>
        <field name="model">types</field>
        <field name="arch" type="xml">
            <form string="Form_types">
                <sheet>
                    <header>
                        <button name="tuto.action_offer_type" type="action" string="Offers" class="oe_stat_button"/>
                    </header> 
                    <group>
                        <field name="name"/>
                    </group>
                    <notebook>
                        <page string="Properties">
                                <field name="property_id" >
                                    <tree>
                                        <field name="name"/>
                                        <field name="expected_price"/>
                                        <field name="state"/>
                                    </tree>
                                </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>