<?xml version="1.0"?>
<odoo>
    <record id="marque_action" model="ir.actions.act_window">
        <field name="name">Marque</field>
        <field name="res_model">marque</field>
        <field name="view_mode">tree,form</field>
    </record>  
    <record id="view_marque_form" model="ir.ui.view">
        <field name="name">marque.form</field>
        <field name="model">marque</field>
        <field name="arch" type="xml">
            <form string="Form">
                <group>
                    <field name="name"/> 
                    <field name="logo" widget="image" options="{'no_open': True}"/>
                </group>
            </form>
        </field>
    </record>
    <record id="view_marque_tree" model="ir.ui.view">
        <field name="name">marque.tree</field>
        <field name="model">marque</field>
        <field name="arch" type="xml">
            <tree string="List">
                    <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>